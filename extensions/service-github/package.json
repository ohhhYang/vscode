{
  "name": "service-github",
  "displayName": "GitHub integration",
  "description": "Provides access to GitHub repositories",
  "categories": [
    "Other"
  ],
  "main": "./out/main",
  "version": "0.0.1",
  "publisher": "sourcegraph",
  "engines": {
    "vscode": "*"
  },
  "activationEvents": [
    "*"
  ],
  "enableProposedApi": true,
  "contributes": {
    "commands": [
      {
        "command": "github.pullRequests.quickopen",
        "title": "%command.pullRequests.quickopen%",
        "category": "GitHub"
      },
      {
        "command": "github.pullRequests.composeLineComment",
        "title": "%command.pullRequests.composeLineComment%",
        "category": "GitHub"
      },
      {
        "command": "github.pullRequest.approve",
        "title": "%command.pullRequest.approve%",
        "category": "GitHub",
        "icon": {
          "light": "resources/icons/light/check.svg",
          "dark": "resources/icons/dark/check.svg"
        }
      },
      {
        "command": "github.pullRequest.requestChanges",
        "title": "%command.pullRequest.requestChanges%",
        "category": "GitHub",
        "icon": {
          "light": "resources/icons/light/x.svg",
          "dark": "resources/icons/dark/x.svg"
        }
      },
      {
        "command": "github.pullRequest.comment",
        "title": "%command.pullRequest.comment%",
        "category": "GitHub",
        "icon": {
          "light": "resources/icons/light/comment.svg",
          "dark": "resources/icons/dark/comment.svg"
        }
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "github.pullRequests.quickopen",
          "when": "config.git.enabled && gitOpenRepositoryCount != 0"
        },
        {
          "command": "github.pullRequests.composeLineComment",
          "when": "githubCanComposeLineComment"
        }
      ],
      "editor/context": [
        {
          "command": "github.pullRequests.composeLineComment",
          "when": "githubCanComposeLineComment",
          "group": "1_github"
        }
      ],
      "scm/title": [
        {
          "command": "github.pullRequests.quickopen",
          "group": "github",
          "when": "config.git.enabled && scmProvider == git"
        }
      ],
      "checklist/itemGroup/context": [
        {
          "command": "github.pullRequest.approve",
          "group": "github"
        },
        {
          "command": "github.pullRequest.comment",
          "group": "github"
        },
        {
          "command": "github.pullRequest.requestChanges",
          "group": "github"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "GitHub",
      "properties": {
        "github.token": {
          "type": "string",
          "description": "%config.token%"
        },
        "github.url": {
          "type": "string",
          "description": "%config.url%"
        },
        "github.cloneProtocol": {
          "type": "string",
          "default": "detect",
          "enum": [
            "detect",
            "https",
            "ssh"
          ],
          "description": "%config.cloneProtocol%"
        },
        "github.debug": {
          "type": "boolean",
          "default": false,
          "description": "%config.debug%"
        }
      }
    }
  },
  "scripts": {
    "compile": "gulp compile-extension:service-github",
    "watch": "gulp watch-extension:service-github",
    "graphql": "get-graphql-schema -j -h \"Authorization=Bearer $GH_TOKEN\" https://api.github.com/graphql | gql2ts -n GitHubGQL -o src/typings/graphqlschema.d.ts"
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "isomorphic-fetch": "^2.2.1",
    "vscode-nls": "^2.0.2"
  },
  "devDependencies": {
    "@sourcegraph/tsconfig": "^2.0.0",
    "@types/mocha": "^2.2.38",
    "@types/node": "^7.0.12",
    "get-graphql-schema": "^2.1.1",
    "gql2ts": "^1.3.0",
    "source-map-support": "^0.5.0",
    "vscode": "1.0.1"
  }
}