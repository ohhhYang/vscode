{
  "name": "xrepo",
  "displayName": "xrepo",
  "description": "Cross repository code navigation (jump-to-def, find refs) using Sourcegraph and LSP",
  "version": "0.0.0",
  "publisher": "Sourcegraph",
  "engines": {
    "vscode": "^1.0.0"
  },
  "main": "./out/extension",
  "categories": [
    "Languages"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "xrepo.initializeWorkspaceFolder",
        "title": "Initialize Workspace Folder Dev Environment"
      },
      {
        "command": "xrepo.downloadDependents",
        "title": "Download dependent sources"
      }
    ],
    "keybindings": []
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./"
  },
  "dependencies": {
    "mz": "^2.7.0",
    "vscode-nls": "^2.0.2"
  },
  "devDependencies": {
    "@sourcegraph/tsconfig": "^1.0.1",
    "@types/mz": "0.0.32"
  }
}
