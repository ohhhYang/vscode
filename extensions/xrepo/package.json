{
  "name": "xrepo",
  "displayName": "xrepo",
  "description": "Cross repository code navigation (jump-to-def, find refs) using Sourcegraph and LSP",
  "version": "0.0.0",
  "publisher": "Sourcegraph",
  "engines": {
    "vscode": "^1.0.0"
  },
  "main": "./out/extension",
  "categories": [
    "Languages"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "xrepo.goToSource",
        "title": "Go to Source File"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "xrepo.goToSource",
          "group": "navigation"
        }
      ]
    },
    "keybindings": []
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./"
  },
  "dependencies": {
    "vscode-nls": "^2.0.2"
  }
}
