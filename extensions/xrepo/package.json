{
  "name": "xrepo",
  "displayName": "xrepo",
  "description": "Cross repository code navigation (jump-to-def, find refs) using Sourcegraph and LSP",
  "version": "0.0.0",
  "publisher": "Sourcegraph",
  "engines": {
    "vscode": "^1.0.0"
  },
  "main": "./out/extension",
  "categories": [
    "Languages"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "xrepo.goToSource",
        "title": "Go to Source File"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "xrepo.goToSource",
          "group": "navigation"
        }
      ]
    },
    "keybindings": []
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "typescript": "^2.5.2",
    "vscode-languageclient": "3.4.0-next.16"
  }
}
